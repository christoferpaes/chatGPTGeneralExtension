{
  "manifest_version": 2,
  "name": "ChatGPT Generalized",
  "version": "1.0",
  "description": "ChatGPT Generalized Extension",
  "permissions": ["activeTab"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
 ,
  "browser_action": {
  
  },
  "manifest_version": 2
}
